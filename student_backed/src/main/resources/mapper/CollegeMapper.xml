<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--namespace是命名空间,唯一标识一个xml文件-->
<!--面向接口,则文件名为接口文件-->
<mapper namespace="com.zretc.student_backed.mapper.CollegeMapper">
    <select id="showCollege" parameterType="java.lang.String" resultType="College">
        select collegeName,deanName,setMajor,introduction
        from college
    </select>
</mapper>
